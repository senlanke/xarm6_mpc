<mujoco model="xarm6">
  <compiler angle="radian" meshdir="assets/" balanceinertia="true"/>

  <option integrator="implicitfast"/>  <!-- 仿真选项：使用implicitfast积分器 -->

  <default>  <!-- 默认属性定义部分 -->
    <default class="xarm6">  <!-- 定义xarm7类的默认属性 -->
      <geom type="mesh" material="white"/>  <!-- 几何体类型为网格，使用白色材质 -->
      <joint axis="0 0 1" armature="0.1" range="-6.28319 6.28319" frictionloss="1"/>  <!-- 关节属性：Z轴旋转，转子惯性0.1，旋转范围±2π，摩擦损失1 在下面没有定义的时候就用默认的-->
      <general biastype="affine" ctrlrange="-6.28319 6.28319"/>  <!-- 通用驱动器：仿射偏置类型，控制范围±2π -->
      <default class="size1">  <!-- 定义size1子类默认属性 -->
        <joint damping="1"/>  <!-- 关节阻尼为10 -->
        <general gainprm="1500" biasprm="0 -1500 -150" forcerange="-50 50"/>  <!-- 通用驱动器参数：增益1500，偏置参数0 -1500 -150，力范围±50 -->
      </default>
      <default class="size2">  <!-- 定义size2子类默认属性 -->
        <joint damping="5"/>  <!-- 关节阻尼为5 -->
        <general gainprm="1000" biasprm="0 -1000 -100" forcerange="-30 30"/>  <!-- 通用驱动器参数：增益1000，偏置参数0 -1000 -100，力范围±30 -->
      </default>
      <default class="size3">  <!-- 定义size3子类默认属性 -->
        <joint damping="2"/>  <!-- 关节阻尼为2 -->
        <general gainprm="800" biasprm="0 -800 -80" forcerange="-20 20"/>  <!-- 通用驱动器参数：增益800，偏置参数0 -800 -80，力范围±20 -->
      </default>
      <site size="0.001" rgba="1 0 0 1" group="4"/>  <!-- 空间点属性：尺寸0.001，红色，组别4 -->
    </default>
  </default>

  <asset>

    <material name="white" rgba="1 1 1 1"/>  <!-- 定义白色材质，RGBA值为1 1 1 1（不透明白） -->
    <material name="gray" rgba="0.753 0.753 0.753 1"/>  <!-- 定义灰色材质，RGBA值为0.753 0.753 0.753 1 ，第四个值为透明度-->
    
    <mesh name="base" file="base.stl"/>
    <mesh name="base_vhacd" file="base_vhacd.obj"/>
    <mesh name="link1" file="link1.stl"/>
    <mesh name="link1_vhacd" file="link1_vhacd.obj"/>
    <mesh name="link2" file="link2.stl"/>
    <mesh name="link2_vhacd" file="link2_vhacd.obj"/>
    <mesh name="link3" file="link3.stl"/>
    <mesh name="link3_vhacd" file="link3_vhacd.obj"/>
    <mesh name="link4" file="link4.stl"/>
    <mesh name="link4_vhacd" file="link4_vhacd.obj"/>
    <mesh name="link5" file="link5.stl"/>
    <mesh name="link5_vhacd" file="link5_vhacd.obj"/>
    <mesh name="link6" file="link6.stl"/>
    <mesh name="link6_vhacd" file="link6_vhacd.obj"/>
  </asset>

  <worldbody>
    <body name="base" pos="0 0 0">
      <inertial pos="0.0 0.0 0.09103" mass="2.7" fullinertia="0.00494875 0.00494174 0.002219 -3.5E-06 1.25E-05 1.67E-06"/>
      <geom name="link_base_visual" type="mesh"  group="1" density="0" mesh="base"/>
      <geom name="link_base_collision" type="mesh" rgba="0.28 0.52 0.92 1" mesh="base_vhacd"/>
      <body name="link1" pos="0 0 0.267" quat="1 0 0 0">`
        <inertial pos="-0.002 0.02692 -0.01332" mass="2.16"  fullinertia="0.00539427 0.0048979 0.00311573 1.095E-05 1.635E-06 0.000793"/>
        <joint name="joint1" axis="0 0 1" range="-6.28319 6.28319" actuatorfrcrange="-50 50" class="size1"/>
        <geom name="link1_visual" type="mesh"  group="1" density="0"  mesh="link1"/>
        <geom name="link1_collision" type="mesh" rgba="0.85 0.19 0.21 1" mesh="link1_vhacd"/>
        <body name="link2" pos="0 0 0" quat="0.707105 -0.707108 0 0">
          <inertial pos="0.03531 -0.21398 0.03386" mass="1.71" fullinertia="0.0248674 0.00485548 0.02387827 -0.00430651 -0.00067797 0.00457245"/>
          <joint name="joint2" axis="0 0 1" range="-2.059 2.0944" actuatorfrcrange="-50 50" class="size1"/>
          <geom name="link2_visual" type="mesh"  group="1" density="0"  mesh="link2"/>
          <geom name="link2_collision" type="mesh" rgba="0.95 0.76 0.05 1" mesh="link2_vhacd"/>
          <body name="link3" pos="0.0535 -0.2845 0" quat="1 0 0 0">
            <inertial pos="0.06781 0.10749 0.01457" mass="1.384" fullinertia="0.0053694 0.0032423 0.00501731 0.0014185 -0.00092094 -0.00169178"/>
            <joint name="joint3" axis="0 0 1" range="-3.927 0.19198" actuatorfrcrange="-32 32" class="size2"/>
            <geom name="link3_visual" type="mesh"  group="1" density="0" mesh="link3"/>
            <geom name="link3_collision" type="mesh" rgba="0.28 0.52 0.92 1" mesh="link3_vhacd"/>
            <body name="link4" pos="0.0775 0.3425 0" quat="0.707105 -0.707108 0 0">
              <inertial pos="-0.00021 0.02578 -0.02538" mass="1.115" fullinertia="0.00439263 0.0040077 0.00110321 5.028E-05 1.374E-05 0.00045338"/>
              <joint name="joint4" axis="0 0 1" range="-6.28319 6.28319" actuatorfrcrange="-32 32" class="size2"/>
              <geom name="link4_visual" type="mesh"  group="1" density="0" mesh="link4"/>
              <geom name="link4_collision" type="mesh" rgba="0.23 0.72 0.32 1" mesh="link4_vhacd"/>
              <body name="link5" pos="0 0 0" quat="0.707105 0.707108 0 0">
                <inertial pos="0.05428 0.01781 0.00543" mass="1.275" fullinertia="0.001202758 0.0022876 0.0026866 0.000492428 -0.00039147 -1.235E-04"/>
                <joint name="joint5" axis="0 0 1" range="-1.69297 3.14159" actuatorfrcrange="-32 32" class="size3"/>
                <geom name="link5_visual" type="mesh"  group="1" density="0" mesh="link5"/>
                <geom name="link5_collision" type="mesh" rgba="0.85 0.19 0.21 1" mesh="link5_vhacd"/>
                <body name="link6" pos="0.076 0.097 0" quat="0.707105 -0.707108 0 0">
                  <inertial pos="0 0.00064 -0.00952" mass="0.1096" fullinertia="4.5293E-05 4.8111E-05 7.9715E-05 0 0 0"/>
                  <joint name="joint6" axis="0 0 1" range="-6.28319 6.28319" actuatorfrcrange="-20 20" class="size3"/>
                  <geom name="link6_visual" type="mesh" group="1" density="0" mesh="link6"/>
                  <geom name="link6_collision" type="mesh" rgba="0.753 0.753 0.753 1" mesh="link6_vhacd"/>
                </body>
              </body>
            </body>
          </body>
        </body>
      </body>
    </body>
  </worldbody>

  <actuator>  <!-- 驱动器定义部分 -->
    <general name="act1" joint="joint1" class="size1"/>  <!-- 通用驱动器：名称act1，驱动joint1，使用size1类属性 -->
    <general name="act2" joint="joint2" class="size1" ctrlrange="-2.059 2.0944"/>  <!-- 通用驱动器：名称act2，驱动joint2，使用size1类属性，控制范围-2.059到2.0944 -->
    <general name="act3" joint="joint3" class="size2" ctrlrange="-3.927 0.19198"/>  <!-- 通用驱动器：名称act4，驱动joint4，使用size2类属性，控制范围-0.19198到3.927 -->
    <general name="act4" joint="joint4" class="size2"/>  <!-- 通用驱动器：名称act5，驱动joint5，使用size2类属性 -->
    <general name="act5" joint="joint5" class="size3" ctrlrange="-1.69297 3.14159"/>  <!-- 通用驱动器：名称act6，驱动joint6，使用size3类属性，控制范围-1.69297到3.14159 -->
    <general name="act6" joint="joint6" class="size3"/>  <!-- 通用驱动器：名称act7，驱动joint7，使用size3类属性 -->
  </actuator>

  <keyframe>  <!-- 关键帧定义部分 -->
    <key name="home" qpos="0 -.247 .909 0 1.15644 0" ctrl="0 -.247 .909 0 1.15644 0"/>  <!-- 关键帧：名称home，关节位置[0,-0.247,0,0.909,0,1.15644,0]，控制值相同 -->
  </keyframe>

</mujoco>
